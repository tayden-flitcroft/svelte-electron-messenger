{
  "name": "svelte-electron-messenger",
  "version": "1.0.0",
  "description": "",
  "main": "./src/electron.ts",
  "scripts": {
    "start": "concurrently -k \"npm:firebase:dev\" \"NODE_ENV=development webpack-dev-server\"",
    "start:electron:dev": "concurrently -k \"npm start\" \"npm:electron:dev\"",
    "build": "webpack",
    "build:electron": "npm run build && electron-forge make",
    "electron:dev": "wait-on tcp:3000 && NODE_ENV=development electron-forge start",
    "firebase:dev": "firebase emulators:start --import=./.firebase_temp --export-on-exit"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@electron-forge/publisher-github": "^6.0.4",
    "@sveltejs/adapter-node": "^1.0.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "dotenv-webpack": "^8.0.1",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-forge": "^5.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^8.31.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "firebase": "^9.15.0",
    "firebase-tools": "^11.20.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss": "^8.4.20",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^2.9.0",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "svelte": "^3.55.0",
    "svelte-loader": "^3.1.4",
    "tailwindcss": "^3.2.4",
    "terser": "^5.16.1",
